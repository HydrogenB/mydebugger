  "scripts": {
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --no-cache --verbose",
    "dev": "vite",
    "build": "vite build",
    "build:vercel": "node vercel-build.js",
    "preview": "vite preview",
    "lint": "eslint \"./**/*.{ts,tsx,js,jsx}\" --max-warnings 0",
    "lint:fix": "eslint \"./**/*.{ts,tsx,js,jsx}\" --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "check": "pnpm lint && pnpm typecheck && pnpm test:coverage",
    "check:ci": "pnpm lint && pnpm typecheck && pnpm test:ci",
    "analyze": "npx unimported && pnpm audit --audit-level moderate",
    "clean": "rimraf dist coverage .cache",
    "clean:deps": "rimraf node_modules pnpm-lock.yaml && pnpm install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "validate": "pnpm clean && pnpm install && pnpm check",
    "precommit": "pnpm lint:fix && pnpm format && pnpm typecheck",
    "prepush": "pnpm check:ci",
    "security": "pnpm audit --audit-level high && npx license-checker --summary",
    "bundle:analyze": "npx bundle-analyzer dist/assets/*.js",
    "docs:generate": "typedoc --out docs/api src/",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && pnpm install"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
